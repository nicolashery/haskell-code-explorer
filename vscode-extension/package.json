{
  "name": "haskell-code-explorer",
  "displayName": "Haskell Code Explorer",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "nicolashery",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexwl/haskell-code-explorer.git"
  },
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.haskell-code-explorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Haskell Code Explorer configuration",
      "properties": {
        "haskellCodeExplorer.host": {
          "type": "string",
          "default": "http://localhost:8080",
          "description": "Host the Haskell Code Explorer server is running on."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.18.0"
  }
}
